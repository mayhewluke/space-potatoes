{
  "name": "space-potatoes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.12",
    "@types/node": "12.0.0",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "bootstrap": "^4.3.1",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1",
    "typescript": "3.4.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json {src,test}/**/*.{ts,tsx} ",
    "typecheck": "tsc --noEmit",
    "verify": "concurrently -n \"LINT,TSC,TEST\" -c \"blue,magenta,yellow\" \"yarn lint\" \"yarn typecheck\" \"CI=true yarn test\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/moxios": "^0.4.8",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^4.3.3",
    "concurrently": "^4.1.0",
    "dom-testing-library": "^4.0.1",
    "husky": "^2.2.0",
    "jest-dom": "^3.2.2",
    "prettier": "^1.17.0",
    "react-testing-library": "^7.0.0",
    "ts-functionaltypes": "^1.0.6",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn verify"
    }
  }
}
